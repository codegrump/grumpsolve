define EquilateralTriangle extends Sketch {
    constructor(parameter side){
        constraint side = distance(p0, p1),
                   side = distance(p1, p2),
                   side = distance(p2, p0);
    }
};

define Pyramid extends Sketch {
    
    parameter side, numLayers;
    
    constructor (parameter side, parameter numLayers) {
        this.side = side;
        this.numLayers = numLayers;
    }
    
    
    
};