#will define x,y who are some radius away from the origin

xy = sketch {
    const radius := 2.0 ;
    
    #declares these variables, seeds them with some random value
    declare x, y;
    
    square(x) + square(y) = square(radius);
};

pointOnCircle = circle {
    x = 2 * y
};

display pointOnCircle;





